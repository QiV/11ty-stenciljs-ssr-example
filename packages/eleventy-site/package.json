{
    "name": "eleventy-site",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "run-p watch serve",
        "serve": "URL=http://localhost:8080 eleventy --serve",        
        "build": "NODE_ENV=production npm run build:css && npm run build:eleventy",
        "build:eleventy": "NODE_ENV=production URL=https://jagreehal.github.io/11ty-stenciljs-ssr-examplezzz eleventy",
        "build:css": "postcss src/tailwind.css -o static/index.css",
        "watch": "postcss src/tailwind.css -o static/index.css --watch"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.11.0",
        "html-minifier": "^4.0.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^7.1.1",
        "postcss-preset-env": "^6.7.0",
        "stencil-web-components": "0.0.1",
        "tailwindcss": "^1.4.6"
    },
    "dependencies": {
        "cpx": "^1.5.0"
    },
    "author": "Jag Reehal",
    "license": "ISC"
}
